int check(Wint n)
{
	Wint sn=sqrt(n);
	return sn*sn==n;
}
int main()
{
	int t;
	for(cin&gt;&gt;t; t--;)
	{
		Wint n;
		cin&gt;&gt;n;
		int ans=check(n)*2+check(n*(n-1)/2);
		cout&lt;&lt;(ans==0?"League of Legends\n":
		       ans==1?"Clash Royale\n":
		       ans==2?"Hearth Stone\n":
		       "Arena of Valor\n");
	}
}
