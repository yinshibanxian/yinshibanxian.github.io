{
	int w,days[13]= {0,12,31,28,31,30,31,30,31,31,30,31,30};
	scanf("%d",&amp;w);
	for(int m=1; m&lt;=12; ++m)
	{
		w=(w+days[m])%7+1;
		if(w==5)
			printf("%d\n",m);
	}
}
