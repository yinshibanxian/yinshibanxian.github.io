<h2 id="flag"><a href="https://vjudge.net/problem/CodeForces-1181C">Flag</a></h2>

<p><code>continue</code>æ‰“é”™æˆ<code>break</code>äº†â€¦å¥½æ°”å•Šã€‚</p>

<pre><code class="language-cpp">#include &lt;bits/stdc++.h&gt;
using namespace std;
const int N = 1023;
char s[N][N];
long long ans;
int n, m, r[N][N], b[N][N];
int getX(int i0, int j)
{
	int i1 = b[i0][j] + 1, len0 = i1 - i0;
	if (i1 &gt;= n)
		return n;
	int i2 = b[i1][j] + 1, len1 = i2 - i1;
	if (i2 &gt;= n || len1 != len0)
		return n;
	if (b[i2][j] &lt; i0 + len0 * 3 - 1)
		return n;
	return i0 + len0 * 3 - 1;
}
int getY(int i0, int j, int x)
{
	int ret = 1e9;
	for (int i = i0; i &lt;= x; ++i)
		ret = min(ret, r[i][j]);
	return ret;
}
int main()
{
	scanf("%d%d", &amp;n, &amp;m);
	for (int i = 0; i &lt; n; ++i)
		scanf("%s", s[i]);
	for (int i = n - 1; ~i; --i)
		for (int j = m - 1; ~j; --j)
		{
			b[i][j] = i;
			if (i + 1 &lt; n &amp;&amp; s[i][j] == s[i + 1][j])
				b[i][j] = b[i + 1][j];
			r[i][j] = j;
			if (j + 1 &lt; m &amp;&amp; s[i][j] == s[i][j + 1])
				r[i][j] = r[i][j + 1];
		}
	for (int i = 0; i &lt; n; ++i)
		for (int j = 0; j &lt; m; ++j)
		{
			int x = getX(i, j);
			if (x &gt;= n)
				continue;
			int y = getY(i, j, x);
			ans += (y - j + 1LL) * (y - j) / 2 + y - j + 1;
			j = y;
		}
	printf("%lld", ans);
}
</code></pre>

