    <pre><code> /**
 * @param {number} n - a positive integer
 * @return {number}
 */
 var hammingWeight = function(n) {
     let nums = 0;
     let temp = n; // å•†
     let ret = n; // ä½™æ•°
     let result = []; // ä¿å­˜è½¬æ¢ä¸ºäºŒè¿›åˆ¶ç»“æœçš„æ•°ç»„
     while(temp &gt; 0) {
         ret = temp % 2;
         temp = Math.floor(temp / 2);
         result.unshift(ret);
     }
     return result.filter(num =&gt; num === 1).length;
 };
