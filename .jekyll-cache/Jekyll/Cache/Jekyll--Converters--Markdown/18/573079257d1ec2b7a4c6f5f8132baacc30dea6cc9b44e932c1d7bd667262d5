<p>ç”±äºåªæœ‰ä¸€ä¸ª magic portalï¼Œå…ˆæŠŠå½“å‰æ‰€åœ¨çš„è¿é€šå—çš„ç³–æœå…¨æ‹¿èµ°ï¼Œä¹‹ ååœ¨ç¬¬ä¸€æ¬¡é‡åˆ°æ•Œäººçš„æ—¶å€™ä½¿ç”¨è‚¯å®šæ˜¯æœ€ä¼˜çš„ã€‚   å…ˆç”¨å¹¶æŸ¥é›†å°†æ²¡æœ‰æ•Œäººçš„ç‚¹åˆå¹¶èµ·æ¥ï¼Œç”¨ä¸€ä¸ªæ•°ç»„å­˜æ”¾è¯¥è¿é€šå—æ‰€å«ç‚¹çš„ æ•°é‡ï¼ˆç§°ä½œæƒå€¼ï¼‰ï¼Œå†ç”¨æœç´¢æœå‡ºå’Œèµ·ç‚¹æ‰€åœ¨è¿é€šå—ç›¸è¿çš„æ€ªç‰©ç‚¹ï¼ŒæŠŠè¿™äº›æ€ª ç‰©ç‚¹æšä¸¾ä¸€ä¸‹ï¼Œéå†è¿æ¥è¿™ä¸ªæ€ªç‰©ç‚¹çš„æ‰€æœ‰è¾¹ï¼Œæ±‚è¾¹çš„å¦ä¸€ç«¯çš„è¿é€šå—çš„æƒå€¼ ï¼ˆ1 è¿™ä¸ªç‚¹æ‰€åœ¨è¿é€šå—çš„æƒå€¼è¦æ”¹ä¸º 0ï¼Œå› ä¸ºå·²ç»æ‹¿è¿‡äº†ï¼‰ï¼Œæ±‚å’Œä¹‹åå†é™¤ä»¥ä¸ è¯¥ç‚¹ç›¸è¿çš„è¾¹æ•°ï¼Œå–æœ€å¤§å€¼ï¼ŒåŠ ä¸Š 1 æ‰€åœ¨è¿é€šå—çš„æƒé‡å³å¯ã€‚ å› ä¸ºé‡åˆ°æ•Œäººæ˜¯éšæœºé€‰æ‹©è¾¹é€ƒè·‘ï¼Œå› æ­¤é‡åˆ°é‡è¾¹çš„æ—¶å€™è¿é€šå—çš„æƒå€¼è¦ç´¯ è®¡å¤šæ¬¡ã€‚åŒæ—¶ä¹Ÿæ˜¯é™¤ä»¥è¿æ¥çš„è¾¹æ•°è€Œä¸æ˜¯è¿æ¥çš„è¿é€šå—æ•°é‡æˆ–è€…æˆ¿é—´æ•°é‡ã€‚æ‹¿ åˆ°çš„ç³–æœæ•°é‡è‡³å°‘æ˜¯ 1 æ‰€åœ¨è¿é€šå—çš„æƒé‡å€¼ï¼Œä¸è®ºç»è¿‡å“ªé‡Œï¼Œéƒ½ä¸€å®šèƒ½æ‹¿åˆ°é‚£ äº›ç³–æœï¼Œæ‰€ä»¥æœ€åç­”æ¡ˆç›´æ¥åŠ ä¸Šå³å¯</p>

<pre><code class="language-cpp">#include &lt;bits/stdc++.h&gt;
using namespace std;
typedef double lf;
struct Graph
{
	struct Vertex
	{
		vector&lt;int&gt; o;
	};
	typedef pair&lt;int, int&gt; Edge;
	vector&lt;Vertex&gt; v; //ç‚¹é›†
	vector&lt;Edge&gt; e;   //è¾¹é›†
	Graph(int n) : v(n) {}
	void add(const Edge &amp;ed)
	{
		v[ed.first].o.push_back(e.size());
		e.push_back(ed);
	}
};
struct UnionfindSet : vector&lt;int&gt;
{
	vector&lt;int&gt; siz;
	UnionfindSet(int n) : vector&lt;int&gt;(n), siz(n, 1)
	{
		for (int i = 0; i &lt; n; ++i)
			at(i) = i;
	}
	void merge(int u, int w)
	{
		if (w = ask(w), u = ask(u), w != u)
			at(w) = u, siz[u] += siz[w];
	}
	int ask(int u) { return at(u) != u ? at(u) = ask(at(u)) : u; }
};
int main()
{
	int t, n, m, k;
	for (scanf("%d", &amp;t); t--;)
	{
		scanf("%d%d%d", &amp;n, &amp;m, &amp;k);
		Graph g(n), h(n);
		UnionfindSet ufs(n);
		vector&lt;int&gt; x(n, 0);
		for (int i = 0, a, b; i &lt; m; ++i)
		{
			scanf("%d%d", &amp;a, &amp;b);
			g.add({a - 1, b - 1});
			g.add({b - 1, a - 1});
		}
		for (int i = 0, t; i &lt; k; ++i)
			scanf("%d", &amp;t), x[t - 1] = 1;
		for (auto e : g.e)
			if (!x[e.first] &amp;&amp; !x[e.second])
				ufs.merge(e.first, e.second);
		lf ans = 0;
		for (int i = 0, adj; i &lt; n; ++i)
			if (x[i])
			{
				lf sum = adj = 0;
				for (auto k : g.v[i].o)
					if (!x[g.e[k].second])
					{
						if (ufs.ask(g.e[k].second) == ufs.ask(0))
							adj = 1;
						else
							sum += ufs.siz[ufs.ask(g.e[k].second)];
					}
				if (adj)
					ans = max(ans, sum / g.v[i].o.size());
			}
		printf("%.9lf\n", ans + ufs.siz[ufs.ask(0)]);
	}
}
