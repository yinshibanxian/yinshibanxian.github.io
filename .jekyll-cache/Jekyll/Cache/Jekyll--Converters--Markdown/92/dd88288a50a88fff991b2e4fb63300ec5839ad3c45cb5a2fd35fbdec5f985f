<h2 id="check-the-text"><a href="https://vjudge.net/problem/Gym-102263C">Check The Text</a></h2>

<pre><code class="language-cpp">#include &lt;bits/stdc++.h&gt;
using namespace std;
const int N = 8e6 + 9;
int n, L, k;
char s[N], st[N], ne[N], op[N];
int main()
{
	scanf("%d", &amp;n);
	for (int i = 1; i &lt;= n; i++)
	{
		scanf("%s", &amp;s);
		int Len = strlen(s);
		for (int j = 0; j &lt; Len; j++)
			st[++L] = s[j];
		st[++L] = ' ';
	}
	L--;
	scanf("%d", &amp;k);
	int now = 0, tp = 0;
	for (int i = 1; i &lt;= k; i++)
	{
		scanf("%s", &amp;op);
		if (op[0] == 'C')
			tp = (tp + 1) % 2;
		else if (op[0] == 'B')
		{
			if (now &gt; 0)
				now--;
		}
		else if (op[0] == 'S')
			ne[++now] = ' ';
		else if (tp == 0)
			ne[++now] = op[0];
		else
			ne[++now] = op[0] - 32;
	}
	bool ok = true;
	if (now != L)
		ok = false;
	for (int i = 1; i &lt;= L; i++)
		if (ne[i] != st[i])
			ok = false;
	printf(ok ? "Correct\n" : "Incorrect\n");
}
</code></pre>

<h2 id="meeting-bahosain"><a href="https://vjudge.net/problem/Gym-102263D">Meeting Bahosain</a></h2>

<pre><code class="language-cpp">#include &lt;bits/stdc++.h&gt;
using namespace std;
typedef long long ll;
const int N = 1e6 + 9;
ll d, a[N], b[N];
int n, m;
int main()
{
	scanf("%d%d", &amp;n, &amp;m);
	for (int i = 0; i &lt; n; ++i)
		scanf("%I64d", &amp;a[i]);
	for (int i = 0; i &lt; m; ++i)
		scanf("%I64d", &amp;b[i]);
	if (m == 1)
		d = b[0];
	else
	{
		d = __gcd(b[0], b[1]);
		for (int i = 2; i &lt; m; ++i)
			d = __gcd(d, b[i]);
	}
	for (int i = 1; i &lt; n; ++i)
		if (abs(a[i] - a[0]) % d)
			return printf("NO"), 0;
	printf("Yes");
}
</code></pre>

