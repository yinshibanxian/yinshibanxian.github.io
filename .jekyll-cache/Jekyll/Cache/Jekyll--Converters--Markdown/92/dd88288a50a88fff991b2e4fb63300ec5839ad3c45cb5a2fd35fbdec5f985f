<h2 id="burgers"><a href="https://vjudge.net/problem/Gym-102263L">Burgers</a></h2>

<pre><code class="language-cpp">#include &lt;bits/stdc++.h&gt;
using namespace std;
typedef long long ll;
const ll M = 1e9 + 7;
ll cal(ll n, ll r)
{
	ll g = __gcd(n, r);
	if (g == 1)
		return (1 + n) * n / 2 % M;
	return cal(n / g, r / g) * g % M;
}
int main()
{
	int n, m, r, c;
	scanf("%d%d%d%d", &amp;n, &amp;m, &amp;r, &amp;c);
	ll k = n / __gcd(n, r), t = m / __gcd(m, c), g = __gcd(k, t);
	cout &lt;&lt; (t / g * cal(n, r) % M * m % M + k / g * cal(m, c) % M + M - k / g * t % M * m % M) % M;
}
</code></pre>

<h2 id="two-operations"><a href="https://vjudge.net/problem/Gym-102263M">Two Operations</a></h2>

<pre><code class="language-cpp">#include &lt;bits/stdc++.h&gt;
using namespace std;
const int N = 1e6 + 9;
char s[N];
int a[31];
int main()
{
	scanf("%s", &amp;s);
	for (int i = 0; s[i]; ++i)
		++a[s[i] - 'a'];
	for (int i = 1; i &lt; 26; ++i)
		a[i] += a[i - 1] &gt;&gt; 1, a[i - 1] &amp;= 1;
	for (char c = 'z'; c &gt;= 'a'; --c)
		cout &lt;&lt; string(a[c - 'a'], c);
}
</code></pre>
