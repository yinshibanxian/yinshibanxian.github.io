<h3 id="Ê∫ê‰ª£Á†Åhelloworldcu">Ê∫ê‰ª£Á†Å<code>HelloWorld.cu</code></h3>

<pre><code class="language-clike">#include &lt;stdio.h&gt;
__global__ void kernel()
{
	printf("Hello world from block(%d,%d,%d) thread(%d,%d,%d).\n",
		   blockIdx.x, blockIdx.y, blockIdx.z,
		   threadIdx.x, threadIdx.y, threadIdx.z);
}
int main()
{
	dim3 grid(2, 4), block(8, 16);
	kernel&lt;&lt;&lt;grid, block&gt;&gt;&gt;();
	cudaDeviceSynchronize();
}
</code></pre>

<h3 id="Ë∞ÉÂ∫¶ËÑöÊú¨helloworldpbs">Ë∞ÉÂ∫¶ËÑöÊú¨<code>HelloWorld.pbs</code></h3>

<pre><code class="language-bash">#PBS -N HelloWorld
#PBS -l nodes=1:ppn=32:gpus=1
#PBS -j oe
#PBS -q gpu

source /public/software/profile.d/cuda10.0.sh
cd $PBS_O_WORKDIR
nvcc HelloWorld.cu -o HelloWorld
./HelloWorld
</code></pre>

