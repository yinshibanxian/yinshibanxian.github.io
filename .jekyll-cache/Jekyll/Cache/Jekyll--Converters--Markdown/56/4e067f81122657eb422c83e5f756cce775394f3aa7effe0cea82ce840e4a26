#include &lt;cstdio&gt;
using namespace std;
const int N = 1e5 + 9;
int n, a[N];
int seven(int k)
{
	if (k % 7 == 0)
		return 1;
	for (; k; k /= 10)
		if (k % 10 == 7)
			return 1;
	return 0;
}
int main()
{
	scanf("%d", &amp;n);
	for (int i = 0, k = 1; i &lt; n; ++k)
	{
		if (seven(k))
			++a[(k - 1) % 4];
		else
			++i;
	}
	for (int i = 0; i &lt; 4; ++i)
		printf("%d\n", a[i]);
	return 0;
}
