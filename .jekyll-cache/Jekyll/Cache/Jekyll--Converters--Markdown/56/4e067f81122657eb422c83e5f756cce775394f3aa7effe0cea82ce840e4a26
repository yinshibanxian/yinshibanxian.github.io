#include &lt;cstdio&gt;
#include &lt;set&gt;
#include &lt;vector&gt;
using namespace std;
int n, cnt[5];
set&lt;pair&lt;int, int&gt;&gt; se;
int main()
{
	scanf("%d", &amp;n);
	for (int i = 0; i &lt; n; ++i)
	{
		pair&lt;int, int&gt; p;
		scanf("%d%d", &amp;p.first, &amp;p.second);
		se.insert(p);
	}
	for (auto p : se)
	{
		vector&lt;pair&lt;int, int&gt;&gt; v(4, p);
		--v[0].first, ++v[1].first, --v[2].second, ++v[3].second;
		if (se.count(v[0]) &amp;&amp; se.count(v[1]) &amp;&amp; se.count(v[2]) &amp;&amp; se.count(v[3]))
		{
			--v[0].second;
			++v[1].second;
			++v[2].first;
			--v[3].first;
			int score = 0;
			for (int i = 0; i &lt; 4; ++i)
				if (se.count(v[i]))
					++score;
			++cnt[score];
		}
	}
	for (int i = 0; i &lt; 5; ++i)
		printf("%d\n", cnt[i]);
	return 0;
}
