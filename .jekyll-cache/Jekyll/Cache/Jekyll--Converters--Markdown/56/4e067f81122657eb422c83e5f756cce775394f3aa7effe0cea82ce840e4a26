<h2 id="é­”æ•°">é­”æ•°</h2>

<pre><code class="language-cpp">// INFO BEGIN
//
// User = 201911505159(å´å)
// Group = C/C++
// Problem = é­”æ•°
// Language = CPP11
// SubmitTime = 2019-12-15 16:09:16
//
// INFO END

#include &lt;cstdio&gt;
using namespace std;
typedef long long ll;
const ll
	N = 1e6 + 9,
	u[5] = {
		314882150829468584,
		427197303358170108,
		1022292690726729920,
		1698479428772363217,
		2006101093849356424},
	fm[2] = {2009731336725594113, 2019};
ll mul(ll a, __int128 b, ll c) { return a * b % c; }
ll f1a[N];
int main()
{
	int n, q;
	scanf("%d%d", &amp;n, &amp;q);
	for (int i = 1; i &lt;= n; ++i)
		f1a[i] = i;
	for (int i = 1, l, r; i &lt;= q; ++i)
	{
		scanf("%d%d", &amp;l, &amp;r);
		int s = 0;
		for (int j = l; j &lt;= r; ++j)
			s += f1a[j] % fm[1];
		printf("%d\n", s);
		ll ut = u[s % 5];
		for (int j = l; j &lt;= r; ++j)
			f1a[j] = mul(f1a[j], ut, fm[0]);
	}
	return 0;
}
