<pre><code class="language-cpp">int main()
{
	int t;
	for(scanf("%d",&amp;t); t--;)
	{
		Coord3 p[6];
		for(int i=0; i&lt;6; ++i)
			scanf("%lf%lf%lf",&amp;p[i].X,&amp;p[i].Y,&amp;p[i].Z);
		printf("%d\n",TriTriIntersection(p,p+3));
	}
}
